<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.docereedemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.docereedemo.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DocereeDemo">

        <activity
            android:name=".DocereeActivity"
            android:label="@string/app_name"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.doceree.androidadslibrary.ads.APPLICATION_ID"
            android:value="fcf41026-d2a6-4f1a-9be5-5eb7161066f6" /> <!-- Test Key -->
    </application>

</manifest>